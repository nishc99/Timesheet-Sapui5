{
  "_version": "1.49.0",
  "sap.app": {
    "id": "com.example.timesheet",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.9.4",
      "toolsId": "aa9787ab-da0a-4563-a49e-a0c40100a995"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.111.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.example.timesheet.i18n.i18n"
        }
      },
      "app": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/local/app.json",
        "preload": true
      },
      "config": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/local/config.json",
        "preload": true
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.example.timesheet.view",
        "controlAggregation": "pages",
        "controlId": "idLayout",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "master",
          "pattern": ":layout:",
          "target": [
            "master"
          ]
        },
        {
          "name": "detail",
          "pattern": "detail/{NOTIFICATION_ID}/{layout}",
          "target": [
            "master",
            "detail"
          ]
        }
      ],
      "targets": {
        "master": {
          "controlAggregation": "beginColumnPages",
          "viewId": "Master",
          "viewName": "Master"
        },
        "detail": {
          "controlAggregation": "midColumnPages",
          "viewId": "Detail",
          "viewName": "Detail"
        }
      }
    },
    "rootView": {
      "viewName": "com.example.timesheet.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
